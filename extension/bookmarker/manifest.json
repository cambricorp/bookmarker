
{
    "description": "Bookmarks",
    "manifest_version": 2,
    "name": "Bookmarker",
    "version": "1.0",
    "homepage_url": "https://github.com/projektir/bookmarker",
    "permissions": [
        "bookmarks",
        "tabs",
        "activeTab",
        "<all_urls>"
    ],
    "browser_action": {
        "browser_style": true,
        "default_popup": "popup/bookmark_actions.html",
        "default_icon": {
            "16": "icons/Pin.svg",
            "32": "icons/Pin.svg"
        },
        "default_title": "Bookmarker"
    }
}
